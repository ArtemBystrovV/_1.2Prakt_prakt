<Page x:Class="_1._2Prakt.Page4"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:_1._2Prakt"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page4">

    <Grid>
        <DataGrid x:Name="BULDING_OBJECTDataGrid" AutoGenerateColumns="False" Margin="0,170,0,151">
            <DataGrid.Columns>
                <DataGridTextColumn Header="ID" Binding="{Binding ID_OBJECT}"/>
                <DataGridTextColumn Header="Worker_ID" Binding="{Binding WORKER_ID}"/>
                <DataGridTextColumn Header="MATERIALS_ID" Binding="{Binding MATERIALS_ID}"/>
                <DataGridTextColumn Header="POSTAVSHIK_ID" Binding="{Binding POSTAVSHIK_ID}"/>
            </DataGrid.Columns>
        </DataGrid>
        <Button Content="Exit" HorizontalAlignment="Left" Margin="456,260,0,0" VerticalAlignment="Top" Height="39" Width="102" Click="ExitButton_Click" RenderTransformOrigin="0.513,0.568"/>
    </Grid>
</Page>
